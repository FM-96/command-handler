{
  "name": "command-handler",
  "version": "0.9.0",
  "description": "Personal command handler for discord.js.",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/index.js",
    "build": "tsc",
    "prepare": "npm run build",
    "dev": "nodemon --exec \"npx ts-node\" src/index.ts",
    "dev-force": "nodemon --exec \"npx ts-node -T\" src/index.ts",
    "check": "npx @biomejs/biome check",
    "fix": "npx @biomejs/biome check --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FM-96/command-handler.git"
  },
