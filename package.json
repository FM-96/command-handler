  },
  "homepage": "https://github.com/FM-96/command-handler#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/node": "^20.14.9",
    "discord.js": "^14.15.3",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  }
}
